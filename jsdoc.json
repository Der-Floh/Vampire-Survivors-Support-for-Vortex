{
  "source": {
    "include": [
      "index.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": false,
    "readme": "./README.md",
    "package": "./info.json",
    "template": "./node_modules/foo-doc"
  },
  "plugins": [
    "plugins/markdown"
  ]
}